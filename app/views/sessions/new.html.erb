
<form action="<%=session_url%>" method="post">

<input type="hidden"
     name="authenticity_token"
     value="<%= form_authenticity_token %>">

<h2> Log in </h2>


<label for="user_name">Username</label>
<input
  type = "text"
  name = "user[user_name]"
  id="user_name"
  value = "">

<label for="user_password">Password</label>
<input
  type="password"
  name="user[password]"
  id="user_password"
  value="">

  <input type="submit" value="Log in">


</form>
